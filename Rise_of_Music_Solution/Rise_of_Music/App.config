<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

    <appSettings>
      <!--
      Input Steam Library Directory (Prefix) between "" (the quote),
      leaving it empty will choose the default location
      e.g.
      <add key="steamLibraryPrefix" value="D:\SteamLibrary\" />
      <add key="steamLibraryPrefix" value="E:\SteamLibrary" />
      -->
      <add key="steamLibraryPrefix" value="" />
    </appSettings>
</configuration>